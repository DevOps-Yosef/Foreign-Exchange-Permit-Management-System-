@model ZB_FEPMS.Models.tblApplicant

<div class="row grid-row">
    <label class="control-label col-sm-2">
        Applicant Name:<span class="required"></span>
    </label>
    <div class="col-sm-10">
        <span class="label-underline-style">
            @Model.ApplicantName
        </span>
    </div>
</div>

<div class="row grid-row">
    <label class="control-label col-sm-2">
        CIF #:<span class="required"></span>
    </label>
    <div class="col-sm-4">
        <span class="label-underline-style">
            @Model.CIFNumber
        </span>
    </div>
</div>

<div class="row grid-row">
    <label class="control-label col-sm-2">
        Remark:<span class="required"></span>
    </label>
    <div class="col-sm-10">
        <span class="label-underline-style">
            @Model.Remark
        </span>
    </div>
</div>

